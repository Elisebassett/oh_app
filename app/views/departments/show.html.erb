<h1><%= @department.name %> Department</h1>
<div class="row">
        <div class="col s12 m3">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Project Info</span>
              <p>project info goes here</p>
            </div>
            <div class="card-action">
            	<%= link_to "Create Project", projects_path %>
            	<br>
            	<%= link_to "Edit Project", edit_project_path %>
            	<%= link_to "Create Project", new_project_path %>
            	<br>
            	<%= link_to "Edit Project", edit_project_path(:id) %>
            </div>
          </div>
        </div>
        <div class="col s12 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Project</span>
              <p>list project here</p>
            </div>
            	<div class="card">
            		<div class="card-content black-text">
              		<span class="card-title">Projects</span>
              			<p><%= link_to "project", project_path(:id) %></p>
              	</div>
            	</div>
          </div>
        </div>
        <div class="col s12 m3">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Team</span>
              <p>Department Manager</p>
            </div>
          	<div class="card blue-grey darken-1">
            	<div class="card-content white-text">
              	<span class="card-title">Employees</span>
              	<p>employees listed</p>
            	</div>
          	</div>	
        	</div>
      </div>