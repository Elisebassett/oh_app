<div class="card col s6">
  <div class="card-content">
    <span class="card-title"><%= department.name %></span>
     <% department.projects.all.each do |project| %>
       <div class="collection">
          <li><%= link_to project.name, project, class: "collection-item" %></li>
        </div>
     <% end %>
  </div>
  <div class="card-action">
    <%= link_to "Edit", edit_department_path(:id) %>
    <br>
    <%# link_to "Delete", method: :destroy, department_path(department) %>
  </div>
</div>

