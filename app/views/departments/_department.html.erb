<div class="card col s6 department">
  <div class="card-content">
    <span class="card-title"><%= department.name %></span>
     <% department.projects.all.each do |project| %>
       <div class="collection">
          <%= link_to project.name, project, class: "collection-item" %>
        </div>
     <% end %>
  </div>
  <div class="card-action">
    <%= link_to "Edit", edit_department_path(:id) %>
    <%= link_to "Delete", departments_path(:id), method: :destroy, remote: :true, class: "float_right destroy_d"%>
  </div>
</div>
