<div class="row">
  <div class="col s6 m6">
    <div class="card">
      <div class="card-content">
        <span class="card-title"><%= department.name %></span>
         <% department.projects.all.each do |project| %>
           <div class="collection">
              <%= link_to project.name, project, class: "collection-item" %>
            </div>
         <% end %>
      </div>
      <div class="card-action">
        <%= link_to "Edit", edit_department_path(department) %>
        <%= link_to "Delete", department_path(department), method: :destroy %>
      </div>
    </div>
  </div>
</div>

